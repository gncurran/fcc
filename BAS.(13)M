function mutation(arr) {
  var result = true;
  var str1 = arr[0].toLowerCase().split('');
  var str2 = arr[1].toLowerCase().split('');

  var bigger = str1.length > str2.length ? str1 : str2;
  var smaller = str1.length > str2.length ? str2 : str1;

  for (var i = 0; i < smaller.length; i++) {
    if (bigger.indexOf(smaller[i]) === -1) {
      result = false;
    }
  }
  
  return result;
}

mutation(["hello", "neo"], "");
